<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-membership-link">
                <arguments>
                    <argument name="path" xsi:type="string">membership</argument>
                    <argument name="label" xsi:type="string" translate="true">My Membership</argument>
                    <argument name="sortOrder" xsi:type="number">205</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
