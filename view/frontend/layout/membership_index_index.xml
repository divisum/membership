    <body>
        <referenceContainer name="content">
            <container name="membership.wrapper" label="Membership Wrapper">
                <block class="Magefox\Membership\Block\Account\Information" name="membership.information" template="Magefox_Membership::account/information.phtml" before="-" cacheable="false" />
            </container>
        </referenceContainer>
    </body>
</page>
